{
    "name": "orklah/psalm-strict-numeric-cast",
    "description": "Restrict the use of (int) and (float) to numeric-string only",
    "type": "psalm-plugin",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "orklah"
        }
    ],
    "extra": {
        "psalm" : {
            "pluginClass": "Orklah\\PsalmStrictNumericCast\\Plugin"
        }
    },
    "require": {
        "php": "^7.3",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Orklah\\PsalmStrictNumericCast\\": ["."],
            "Orklah\\PsalmStrictNumericCast\\Hooks\\": ["hooks"]
        }
    },
    "require-dev": {
        "vimeo/psalm": "^4.0",
        "nikic/php-parser": "^4.0"
    }
}
